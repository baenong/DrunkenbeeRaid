    "express-flash": "^0.0.2",
    "express-session": "^1.17.3",
    "mongoose": "^6.8.0",
    "node-cron": "^3.0.2",
    "pug": "^3.0.2",
    "socket.io": "^4.6.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.20.5",
    "@babel/node": "^7.20.5",
    "@babel/preset-env": "^7.20.2",
